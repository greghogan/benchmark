# rand write - measures max write IOPS with 4k blocks

[global]
rw=randwrite
ioengine=libaio

bs=4k
nrfiles=1

direct=1
sync=0
end_fsync=1
iodepth=128
invalidate=1
refill_buffers
verify=0

[job0]
filename=/dev/disk/by-id/google-local-nvme-ssd-0

[job1]
filename=/dev/disk/by-id/google-local-nvme-ssd-1

[job2]
filename=/dev/disk/by-id/google-local-nvme-ssd-2

[job3]
filename=/dev/disk/by-id/google-local-nvme-ssd-3

[job4]
filename=/dev/disk/by-id/google-local-nvme-ssd-4

[job5]
filename=/dev/disk/by-id/google-local-nvme-ssd-5

[job6]
filename=/dev/disk/by-id/google-local-nvme-ssd-6

[job7]
filename=/dev/disk/by-id/google-local-nvme-ssd-7

